<UserControl x:Class="PacChat.Resources.CustomControls.Dialogs.WaitingDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PacChat.Resources.CustomControls.Dialogs"
             mc:Ignorable="d" Height="100" Width="100">
    <DockPanel>
        <ProgressBar
          Style="{StaticResource MaterialDesignCircularProgressBar}"
          Value="0"
          IsIndeterminate="True" Height="Auto" Margin="20" Width="Auto" Background="{x:Null}" BorderBrush="{x:Null}" /> 
    </DockPanel>
</UserControl>
